{% extends "html.j2" %}

{% block content %}
    <div class="left-column">
      <div>
        <h1>{{ title }}</h1>
        {% for page in past.pages %}
          <p>{{ format_date(page.from) }} - {{ format_date(page.to) }}: <a href="{{ page.link }}">{{ page.title }}</a></p>
        {% endfor %}
      </div>
    </div>
{% endblock %}